{
    "name": "ijodkor/laravel-generator",
    "description": "Laravel CRUD API generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anvar Zaripboyev",
            "email": "mendasturchiman@gmail.com"
        },
        {
            "name": "Dilmurod Qurbonboyev",
            "email": "qurbonboyev9820@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "ijodkor/laravel-api-response": "^1.2",
        "illuminate/contracts": "^9.21|^10.0|^11.0",
        "illuminate/queue": "^9.21|^10.0|^11.0",
        "illuminate/support": "^9.21|^10.0|^11.0",
        "livewire/livewire": "^3.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Uzinfocom\\LaravelGenerator\\": "src/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "scripts": {
        "lint": [
            "@php vendor/bin/phpstan analyse"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uzinfocom\\LaravelGenerator\\MainServiceProvider"
            ],
            "aliases": {
                "LaravelGenerator": "Uzinfocom\\LaravelGenerator"
            }
        }
    }
}
